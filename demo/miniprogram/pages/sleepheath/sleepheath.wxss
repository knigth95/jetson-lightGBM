page {
  background:#f0cdd8;
  width: 100%;
  height: 100%;
}
/* 样式表，根据需要进行自定义 */
.container {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  background-color: #ccaeb8;
}

.title {
  font-size: 20px;
  font-weight: bold;
  margin-bottom: 20px;
}

.sleep-info {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.sleep-icon {
  width: 100px;
  height: 100px;
  margin-bottom: 10px;
  border-radius: 100%;
  overflow: hidden;
  margin-bottom: 10px;
}

.sleep-duration,
.sleep-change {
  font-size: 16px;
  margin-bottom: 10px;
}
.date-button {
  padding: 10px;
  font-size: 16px;
  border: 1px solid #ccaeb8;
  border-radius: 4px;
  background-color: #ccaeb8;
}

.picker-value {
  padding: 10px;
  font-size: 16px;
}
.date-view{
  border-radius: 100rpx; /* 设置边框的圆角大小 */
  overflow: hidden; /* 隐藏超出边界的内容 */
}
.module {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.number {
  font-size: 20px;
  color: #fff;
  font-weight: bold;
}

.text {
  font-size: 20px;
  margin-top: 5px;
  color: #fff;
}
.title{
  font-size: 16px;
  margin-top: 5px;
  color: #fff;
}
.number1{
  font-size: 22px;
  color: #fff;
  font-weight: bold;
}
.text1{
  font-size: 12px;
  margin-top: 5px;
  color: rgb(124, 73, 73);
}